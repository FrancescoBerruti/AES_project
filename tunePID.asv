clear
s=tf('s');
Tau=1;
TauHC=1/10;
TauZero=10;

L=1/Tau*(1+(TauZero)*s)*(1+TauHC*s)/(TauZero*s*(1+TauHC*s)*s);
P=1/Tau*(1+(TauZero)*s)/(TauZero*s*(1+TauHC*s))*80000;
pole(L)
margin(L)

